test:
  adapter: mysql2
  host: 127.0.0.1
  encoding: utf8mb4
  username: root
  database: nctuplus_test

development:
  adapter: mysql2
  url: <%= ENV['DATABASE_URL'] %>
  encoding: utf8mb4
  database: nctuplus_development
  host: <%= ENV['DATABASE_HOST'] %>
  username: { DEVELOP_USER_NAME }
  password: { DEVELOP_USER_PASSWD }

production:
  adapter: mysql2
  encoding: utf8mb4
  database: nctuplus
  host: localhost
  username: { PROD_USER_NAME }
  password: { PROD_USER_PASSWD }
